{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/no-multi-comp": "off",
    "react/no-did-mount-set-state": "off",
    "import/prefer-default-export": "off"
  },
  "plugins": ["jest"]
}
